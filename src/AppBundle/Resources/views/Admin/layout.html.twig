{% extends '::base.html.twig' %}

{% block stylesheets %}
    <link href="{{ asset('/bower_components/metisMenu/dist/metisMenu.min.css') }}" rel="stylesheet">
    <link href="{{ asset('/bower_components/startbootstrap-sb-admin-2/dist/css/sb-admin-2.css') }}" rel="stylesheet">
    <link href="{{ asset('/bower_components/angular-chosen-localytics/chosen-spinner.css') }}" rel="stylesheet">
    <link href="{{ asset('/bower_components/angular-bootstrap/ui-bootstrap-csp.css') }}" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('/bower_components/angular-spinkit/build/angular-spinkit.min.css') }}">

    <link href="{{ asset('/bundles/app/css/admin.css')}}" rel="stylesheet" type="text/css">
{% endblock %}
{% block body %}
<div id="wrapper" ng-app="eveTool">
    <!-- Navigation -->
    {% include '::common/navigation.html.twig' %}

    <!-- Page Content -->
    <div id="page-wrapper" >
        {#
        <div id="page-overlay"></div>
        #}
        <div class="container-fluid" >
            {%  include ':common:flashes.html.twig' %}
            {% block content %}
                {% block fos_user_content %}
                {% endblock fos_user_content %}
            {% endblock %}
            <!-- /.row -->
            {% include '::common/footer.html.twig' %}
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->

</div>
{% endblock %}
{% block extra_javascript %}
    {{ parent() }}

